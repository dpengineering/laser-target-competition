<TargetScreen>:
    #: import time __main__.time

    name: 'target'
    FloatLayout:
        id: float
        size_hint: None, None

    ImageButton:
        id: start
        source: '../../assets/images/buttons/start.png'
        center_x: root.width / 2
        center_y: root.height / 2 - 128
        size: 216, 44
        on_press: root.start()

    DPEAButton:
        id: test
        text: "TEST"
        size: 200, 65
        x: 0
        y: 0
        color: 0, 1, 1, 1
        on_press: root.test()
        font_name: 'PixelFont'
        font_size: '20sp'

    DPEAButton:
        id: exit
        text: "EXIT"
        size: 200, 65
        x: 0
        y: 0
        color: 0, 1, 1, 1
        on_press: root.exit()
        font_name: 'PixelFont'
        font_size: '20sp'



    DPEAButton:
        id: left
        text: "left"
        center_x: root.width / 2 + 100
        center_y: root.height / 2 - 200
        size: 216, 44
        on_press: root.left()

    DPEAButton:
        id: right
        text: "right"
        center_x: root.width / 2 - 100
        center_y: root.height / 2 - 200
        size: 216, 44
        on_press: root.right()

